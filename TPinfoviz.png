import requests

# URL of the static PNG image from your Tableau visualization
image_url = 'https://public.tableau.com/static/images/4S/4S3S89Y27/1.png'

# Send a GET request to fetch the image content
response = requests.get(image_url)

# Check if the request was successful
if response.status_code == 200:
    # Save the image content to a file
    with open('tableau_visualization.png', 'wb') as f:
        f.write(response.content)
    print('Image downloaded successfully: tableau_visualization.png')
else:
    print('Failed to retrieve the image. Status code:', response.status_code)
